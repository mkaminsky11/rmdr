{
  "name": "rmdr",
  "description": "Quickly make reminders in the terminal",
  "version": "0.2.0",
  "homepage": "https://github.com/mkaminsky11/rmdr",
  "os" : [ "darwin", "linux" ],
  "author": {
    "name": "Michael Kaminsky",
    "email": "mkaminsky11@github.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mkaminsky11/rmdr.git"
  },
  "bugs": {
    "url": "https://github.com/mkaminsky11/rmdr/issues"
  },
  "license": "MIT",
  "main": "lib/rmdr",
  "engines": {
    "node": ">=0.10"
  },
  "dependencies": {
  	"crontab": "*",
  	"shortid": "*",
  	"cli-table2": "*"
